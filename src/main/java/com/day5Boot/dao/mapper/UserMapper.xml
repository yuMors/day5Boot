<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.day5Boot.dao.UserMapper">
    <select id="findMyUser" resultType="com.day5Boot.domain.User">
        select * from user
    </select>
    <select id="findUserById" resultType="com.day5Boot.domain.User">
        select * from user where id = #{id}
    </select>
    <!--#{} 不会参与预编译 ${} 参与预编译-->
    <select id="findUserByWrapper" resultType="com.day5Boot.domain.User">
        select * from user ${ew.customSqlSegment}
    </select>
</mapper>